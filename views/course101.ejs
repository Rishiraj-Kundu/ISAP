        function markAsRead() {
            if (!completedModules.includes(currentModule)) {
                completedModules.push(currentModule);
                updateProgressBar();
                updateModuleList();
                showNextModuleButton();
            }
        }

        function showNextModuleButton() {
            const nextModuleBtn = document.querySelector('.next-module-btn');
            nextModuleBtn.style.display = 'block';
        }

        function loadNextModule() {
            if (currentModule < completedModules.length + 1) {
                currentModule++;
                loadModule(currentModule);
            }
        }

        function updateProgressBar() {
            const progressBar = document.getElementById('progressBar');
            const moduleList = document.getElementById('moduleList');
            const progress = (completedModules.length / moduleList.children.length) * 100;
            progressBar.style.width = progress + '%';
            progressBar.innerHTML = progress.toFixed(0) + '%';
        }

        function updateModuleList() {
            const moduleList = document.getElementById('moduleList');
            for (let i = 1; i <= moduleList.children.length; i++) {
                const moduleItem = document.getElementById(`module${i}`);
                moduleItem.classList.remove('active', 'disabled');
                if (completedModules.includes(i)) {
                    moduleItem.classList.add('active');
                } else {
                    moduleItem.classList.add('disabled');
                }
            }
        }

        document.addEventListener('DOMContentLoaded', () => {
            updateModuleList();
        });

