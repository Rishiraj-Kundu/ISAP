      const seconds = timeLeft % 60;
      document.getElementById('countdown').innerHTML = `${String(minutes).padStart(2, '0')}:${String(seconds).padStart(2, '0')}`;
    }

    // Function to decrement the timer and submit the form when the time is up
    function countdown() {
